{
  "extends": "@dms/typescript-config/library.json",
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "ES2023"
    ],
    "jsx": "react-jsx",
    "outDir": "./dist",
    "rootDir": "./src",
    "incremental": false,
    "composite": false,
    "verbatimModuleSyntax": false
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}
