{
  "name": "@dms/api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "nest build",
    "dev": "nest start --watch",
    "start": "node dist/main",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "test": "vitest run --passWithNoTests",
    "test:unit": "vitest run --dir src",
    "test:integration": "vitest run --dir test/integration",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "lint": "eslint \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" \"test/**/*.ts\" --fix",
    "type-check": "tsc --noEmit",
    "db:migrate": "prisma migrate dev --schema=../../prisma/schema.prisma",
    "db:generate": "prisma generate --schema=../../prisma/schema.prisma",
    "db:push": "prisma db push --schema=../../prisma/schema.prisma",
    "db:seed": "dotenv -e .env -- tsx ../../prisma/seed.ts",
    "db:studio": "prisma studio --schema=../../prisma/schema.prisma",
    "clean": "rm -rf dist .turbo coverage"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.700.0",
    "@aws-sdk/client-textract": "^3.700.0",
    "@aws-sdk/s3-request-presigner": "^3.700.0",
    "@dms/shared": "workspace:*",
    "@nestjs/bullmq": "^10.2.0",
    "@nestjs/common": "^11.0.0",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^11.0.0",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.0",
    "@nestjs/platform-express": "^11.0.0",
    "@nestjs/platform-socket.io": "^11.0.0",
    "@nestjs/swagger": "^8.1.0",
    "@nestjs/throttler": "^6.3.0",
    "@nestjs/websockets": "^11.0.0",
    "@pdf-lib/fontkit": "^1.1.1",
    "@prisma/client": "5.20.0",
    "@socket.io/redis-adapter": "^8.3.0",
    "@types/bcrypt": "^6.0.0",
    "archiver": "^7.0.1",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "bullmq": "^5.30.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "helmet": "^8.0.0",
    "ioredis": "^5.4.0",
    "joi": "^17.13.0",
    "nodemailer": "^7.0.12",
    "passport": "^0.7.0",
    "passport-custom": "^1.1.1",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "passport-microsoft": "^2.0.0",
    "pdf-lib": "^1.17.1",
    "pdf-parse": "^2.4.5",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "sharp": "^0.33.5",
    "socket.io": "^4.8.0",
    "uuid": "^11.0.0",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@dms/eslint-config": "workspace:*",
    "@dms/typescript-config": "workspace:*",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.0",
    "@types/archiver": "^7.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.0",
    "@types/nodemailer": "^7.0.5",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/pdf-parse": "^1.1.5",
    "@types/uuid": "^10.0.0",
    "@vitest/coverage-v8": "^2.1.0",
    "prisma": "5.20.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "tsx": "^4.19.0",
    "typescript": "^5.9.0-beta",
    "vitest": "^2.1.0"
  }
}
